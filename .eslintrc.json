{
    "extends": [
        "react-app",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "prettier/prettier": "error"
    },
    "plugins": [
        "prettier"
    ],
    "env": {
        "es6": true
    }
}